<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hotel.po.Room">
	<select id="getAllRoom" resultType="Room">
		SELECT * FROM room
	</select>
	<insert id="addRoom" parameterType="Room">
		INSERT INTO room (r_num,r_size,r_typeid,r_isbath,r_monry)
			VALUES(#{r_num},#{r_size},#{r_typeid},#{r_isbath},#{r_monry})
	</insert>
</mapper>